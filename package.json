{
  "name": "@socialcap/semaphore-sdk",
  "version": "0.1.1",
  "description": "An 'o1js' implementation of the Semaphore protocol",
  "author": "maz@socialcap.app",
  "license": "Apache-2.0",
  "keywords": [
    "socialcap",
    "semaphore",
    "semaphore-sdk",
    "mina",
    "minaprotocol",
    "o1js",
    "mina-zkapp",
    "mina-zk",
    "zkapp"
  ],
  "type": "module",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
